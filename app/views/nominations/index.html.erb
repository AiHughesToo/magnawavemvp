<p id="notice"><%= notice %></p>

<h1>Nominations</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Senderid</th>
      <th>Sendername</th>
      <th>Receiverid</th>
      <th>Receivername</th>
      <th>Receiverimage</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nominations.each do |nomination| %>
      <tr>
        <td><%= nomination.type %></td>
        <td><%= nomination.senderId %></td>
        <td><%= nomination.senderName %></td>
        <td><%= nomination.receiverId %></td>
        <td><%= nomination.receiverName %></td>
        <td><%= nomination.receiverImage %></td>
        <td><%= nomination.body %></td>
        <td><%= link_to 'Show', nomination %></td>
        <td><%= link_to 'Edit', edit_nomination_path(nomination) %></td>
        <td><%= link_to 'Destroy', nomination, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nomination', new_nomination_path %>
